    <!-- http://www.w3.org/ns/org#&#39;has_member&#39; -->

    <owl:ObjectProperty rdf:about="&org;&#39;has_member&#39;">
        <rdfs:domain rdf:resource="&org;Organisation"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/org#&#39;member_of&#39; -->

    <owl:ObjectProperty rdf:about="&org;&#39;member_of&#39;">
