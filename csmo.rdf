        <terms:creator>Armand Brahaj</terms:creator>
        <owl:versionInfo>0.2</owl:versionInfo>
        <terms:description xml:lang="en">An ontology for the representation of scientific study metadata based on the Core Scientific Metadata Model.</terms:description>
