            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Investigator describes the people involved in the study together with their institution and role in the study.</rdfs:comment>
    </owl:Class>
    


    <!-- https://www.escidoc.org/ontologies/csmo.owl#Measurement -->

    <owl:Class rdf:about="&csmo;Measurement">
