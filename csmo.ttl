<https://www.escidoc.org/ontologies/csmo/> rdf:type owl:Ontology ;
                                           
                                           terms:creator "Armand Brahaj" ;
                                           
                                           owl:versionInfo "0.2" ;
                                           
                                           terms:description "An ontology for the representation of scientific study metadata based on the Core Scientific Metadata Model."@en ;
                                           
                                           terms:publisher "http://www.escidoc.org"@en ;
                                           
                                           owl:versionIRI <https://www.escidoc.org/ontologies/csmo/> .


#################################################################
#
#    Annotation properties
#
#################################################################


terms:creator rdf:type owl:AnnotationProperty .



terms:description rdf:type owl:AnnotationProperty .



terms:hasVersion rdf:type owl:AnnotationProperty .



terms:publisher rdf:type owl:AnnotationProperty .



terms:modified rdf:type owl:AnnotationProperty .



skos:note rdf:type owl:AnnotationProperty .



terms:issued rdf:type owl:AnnotationProperty .



terms:title rdf:type owl:AnnotationProperty ;
            
            rdfs:label "title"@en ;
            
            rdfs:comment "A name given to the resource."@en ;
            
            rdfs:isDefinedBy terms: .





