###  http://www.w3.org/ns/org#'has_member'

org:'has_member' rdf:type owl:ObjectProperty ;
                 
                 rdfs:domain org:Organisation ;
                 
                 rdfs:range foaf:Person .



###  http://www.w3.org/ns/org#'member_of'

org:'member_of' rdf:type owl:ObjectProperty ;
                
                owl:inverseOf org:'has_member' ;
                
                rdfs:range org:Organisation ;
                
                rdfs:domain foaf:Agent .



###  http://www.w3.org/ns/org#hasSite

org:hasSite rdf:type owl:ObjectProperty ;
            
            rdfs:range org:Site ;
            
            rdfs:domain csmo:#Institution ,
                        csmo:#Instrument .



###  https://www.escidoc.org/ontologies/csmo/#hasAccess

csmo:#hasAccess rdf:type owl:ObjectProperty ;
                
                rdfs:label "hasAccess" ;
                
                rdfs:range nfo:Folder ;
                
                rdfs:domain org:Role ;
                
                rdfs:range csmo:#Investigation .



