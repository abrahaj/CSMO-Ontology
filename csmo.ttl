#################################################################
#
#    Classes
#
#################################################################


###  http://purl.org/ontology/bibo/Document

bibo:Document rdf:type owl:Class ;
              
              owl:equivalentClass csmo:#Publication .



###  http://www.geonames.org/ontology#Feature

gn:Feature rdf:type owl:Class ;
           
           rdfs:label "Location"@en ;
           
           rdfs:comment "A geographical object uniquely defined by its geonames id."@en ;
           
           skos:note "Original Label gn:Feature"@en .



###  http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#Folder

nfo:Folder rdf:type owl:Class ;
           
           rdfs:label "Dataset"@en ;
           
           rdfs:subClassOf owl:Thing ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty csmo:#hasDataSetStatus ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty csmo:#hasDataSetType ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty csmo:#hasDataset ;
                             owl:someValuesFrom nfo:Folder
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty csmo:#hasSample ;
                             owl:onClass csmo:#Sample ;
                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger
                           ] ;
           
           rdfs:comment "A resource containing a finite sequence of bytes with arbitrary information, that is available to a computer program and is usually based on some kind of durable storage. A file is durable in the sense that it remains available for programs to use after the current program has finished."@en ;
           
           terms:description """Datafile 
The CSMD takes a hierarchical view of data holdings, as data sets may contain other dataset as well as units of storage, typically datafiles. Each datafile has more detailed information, including its name, version, location, data format, creation and modification time, and fixity information such as a Checksum. """@en ;
           
           rdfs:isDefinedBy nfo:Folder .



###  http://www.w3.org/ns/org#Organisation

org:Organisation rdf:type owl:Class ;
                 
                 owl:equivalentClass csmo:#Institution .



###  http://www.w3.org/ns/org#Role

org:Role rdf:type owl:Class ;
         
         rdfs:label "Role"@en ;
         
         rdfs:subClassOf owl:Thing ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty csmo:#hasAccess ;
                           owl:someValuesFrom nfo:Folder
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty csmo:#hasAccess ;
                           owl:someValuesFrom csmo:#Investigation
                         ] ;
         
         rdfs:comment "Denotes a role that a Person or other Agent can take in an organization."@en ;
         
         rdfs:isDefinedBy "org:Role" ;
         
         skos:note "sameAs foaf:Group" .



###  http://www.w3.org/ns/org#Site

org:Site rdf:type owl:Class ;
         
         rdfs:label "Site"@en ;
         
         owl:equivalentClass csmo:#Facility ;
         
         rdfs:subClassOf owl:Thing ;
         
         rdfs:comment "An office or other premise at which the study/investigation takes place is located. Many studies/investigations are spread across multiple sites. In most cases a Site will be a physical location."@en .



###  http://xmlns.com/foaf/0.1/Agent

foaf:Agent rdf:type owl:Class .



###  http://xmlns.com/foaf/0.1/Person

foaf:Person rdf:type owl:Class ;
            
            owl:equivalentClass csmo:#Investigator ;
            
            rdfs:subClassOf foaf:Agent ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty org:'member_of' ;
                              owl:someValuesFrom csmo:#Institution
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty csmo:#hasRole ;
                              owl:someValuesFrom org:Role
                            ] ;
            
            rdfs:isDefinedBy "foaf:Person" .



###  https://www.escidoc.org/ontologies/csmo/#AnalysedResult

csmo:#AnalysedResult rdf:type owl:Class ;
                     
                     owl:equivalentClass csmo:#DerivedDataResult ;
                     
                     rdfs:subClassOf csmo:#Result ;
                     
                     terms:description "Curated and analysed data file"@en .



###  https://www.escidoc.org/ontologies/csmo/#AtomicDataObject

csmo:#AtomicDataObject rdf:type owl:Class ;
                       
                       rdfs:label "AtomicDataObject"@en ;
                       
                       rdfs:subClassOf nfo:Folder .



###  https://www.escidoc.org/ontologies/csmo/#AtomicDataObjectParameter

csmo:#AtomicDataObjectParameter rdf:type owl:Class ;
                                
                                rdfs:label "AtomicDataObjectParameter"@en ;
                                
                                rdfs:subClassOf csmo:#Parameter ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty csmo:#isADOParameter ;
                                                  owl:someValuesFrom csmo:#AtomicDataObject
                                                ] .



###  https://www.escidoc.org/ontologies/csmo/#CalibrationData

csmo:#CalibrationData rdf:type owl:Class ;
                      
                      rdfs:label "CalibrationData"@en ;
                      
                      rdfs:subClassOf csmo:#Result .



###  https://www.escidoc.org/ontologies/csmo/#DataFile

csmo:#DataFile rdf:type owl:Class ;
               
               rdfs:label "DataFile"@en ;
               
               owl:equivalentClass csmo:#Result ;
               
               rdfs:subClassOf nfo:Folder ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty csmo:#creationDate ;
                                 owl:someValuesFrom xsd:dateTime
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty csmo:#version ;
                                 owl:someValuesFrom rdfs:Literal
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty csmo:#fixity ;
                                 owl:someValuesFrom xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty csmo:#hasLocation ;
                                 owl:someValuesFrom gn:Feature
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty terms:title ;
                                 owl:cardinality "1"^^xsd:nonNegativeInteger
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty csmo:#modificationDate ;
                                 owl:someValuesFrom xsd:dateTime
                               ] ;
               
               rdfs:comment "Each datafile has more detailed information, including its name, version, location, data format, creation and modification time, and fixity information such as a Checksum."@en .



###  https://www.escidoc.org/ontologies/csmo/#DataSetParameter

csmo:#DataSetParameter rdf:type owl:Class ;
                       
                       rdfs:label "DataSetParameter"@en ;
                       
                       rdfs:subClassOf csmo:#Parameter .



###  https://www.escidoc.org/ontologies/csmo/#DerivedDataResult

csmo:#DerivedDataResult rdf:type owl:Class ;
                        
                        rdfs:subClassOf csmo:#Result .



###  https://www.escidoc.org/ontologies/csmo/#EndResult

csmo:#EndResult rdf:type owl:Class ;
                
                rdfs:subClassOf csmo:#Result .



###  https://www.escidoc.org/ontologies/csmo/#Experiment

csmo:#Experiment rdf:type owl:Class ;
                 
                 rdfs:label "Experiment"@en ;
                 
                 rdfs:subClassOf csmo:#Investigation ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty csmo:#hasInstrument ;
                                   owl:someValuesFrom csmo:#Instrument
                                 ] ;
                 
                 rdfs:comment "Experiments. Investigations into the physical behaviour of the environment usually to test a hypothesis, typically involving an instrument operating under some instrumental settings and environmental conditions, and generating datasets in files. "@en .



###  https://www.escidoc.org/ontologies/csmo/#Facility

csmo:#Facility rdf:type owl:Class ;
               
               rdfs:subClassOf owl:Thing .



###  https://www.escidoc.org/ontologies/csmo/#Institution

csmo:#Institution rdf:type owl:Class ;
                  
                  rdfs:label "Institution"@en ;
                  
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty org:hasSite ;
                                    owl:onClass org:Site ;
                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                  ] .



###  https://www.escidoc.org/ontologies/csmo/#Instrument

csmo:#Instrument rdf:type owl:Class ;
                 
                 rdfs:subClassOf owl:Thing ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty csmo:#isInstrumentOf ;
                                   owl:allValuesFrom csmo:#Facility
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty csmo:#settings ;
                                   owl:cardinality "1"^^xsd:nonNegativeInteger
                                 ] .



###  https://www.escidoc.org/ontologies/csmo/#Investigation

csmo:#Investigation rdf:type owl:Class ;
                    
                    rdfs:label "Investigation"@en ;
                    
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty terms:title ;
                                      owl:cardinality "1"^^xsd:nonNegativeInteger
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty csmo:#hasInvestigator ;
                                      owl:onClass csmo:#Investigator ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty csmo:#hasTopic ;
                                      owl:someValuesFrom rdfs:Literal
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty csmo:#hasPrincipalInvestigator ;
                                      owl:onClass csmo:#Investigator ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty csmo:#isInvestigationOf ;
                                      owl:onClass csmo:#Study ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty csmo:#abstract ;
                                      owl:cardinality "1"^^xsd:nonNegativeInteger
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty csmo:#hasKeyword ;
                                      owl:someValuesFrom rdfs:Literal
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty csmo:#hasResult ;
                                      owl:someValuesFrom csmo:#Result
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty csmo:#hasInstrument ;
                                      owl:someValuesFrom csmo:#Instrument
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty csmo:#hasData ;
                                      owl:someValuesFrom csmo:#Result
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty csmo:#hasSample ;
                                      owl:someValuesFrom csmo:#Sample
                                    ] ;
                    
                    rdfs:comment "Investigation: forms the fundamental unit of the model, with a title, abstract, dates, and unique identifiers referencing the particular study. Also associated with the investigation are the facility and instrument used to collect data. "@en .



###  https://www.escidoc.org/ontologies/csmo/#Investigator

csmo:#Investigator rdf:type owl:Class ;
                   
                   rdfs:label "Investigator"@en ;
                   
                   rdfs:subClassOf org:Role ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty csmo:#memberOf ;
                                     owl:someValuesFrom csmo:#Institution
                                   ] ;
                   
                   rdfs:comment "Investigator describes the people involved in the study together with their institution and role in the study."@en .



###  https://www.escidoc.org/ontologies/csmo/#Measurement

csmo:#Measurement rdf:type owl:Class ;
                  
                  rdfs:label "Measurement"@en ;
                  
                  rdfs:subClassOf csmo:#Investigation ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty csmo:#hasInstrument ;
                                    owl:someValuesFrom csmo:#Instrument
                                  ] ;
                  
                  rdfs:comment "Measurements. Investigations that record the state of some aspect of the environment over a sequence of points in time and space, using some passive detector, e.g., the measurement of temperature at a point on the earth surface taken hourly using a thermometer of known accuracy."@en .



###  https://www.escidoc.org/ontologies/csmo/#Observation

csmo:#Observation rdf:type owl:Class ;
                  
                  rdfs:subClassOf csmo:#Investigation .



###  https://www.escidoc.org/ontologies/csmo/#Parameter

csmo:#Parameter rdf:type owl:Class ;
                
                rdfs:label "Parameter"@en ;
                
                rdfs:comment "Parameters can be associated with data holdings, datasets, or ADOs. The same metadata item is used to represent either experimental conditions or measured items stored as data points in the data collection, but are distinguished via a parameter type qualifier (‘fixed’ or ‘measured’).Each parameter has a set of fields describing its name (e.g., temperature, pressure), its value (if fixed in as an input parameter), the units of measurement used to qualify the data points, the range of values over-which a parameter can take and the error margin expected on the value."@en .



###  https://www.escidoc.org/ontologies/csmo/#Policy

csmo:#Policy rdf:type owl:Class ;
             
             rdfs:label "Policy"@en ;
             
             rdfs:subClassOf owl:Thing ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty csmo:#policyCreatedBy ;
                               owl:someValuesFrom csmo:#Institution
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty csmo:#hasProgramme ;
                               owl:someValuesFrom csmo:#Programme
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty terms:title ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger
                             ] ;
             
             rdfs:comment "Policy. The company or government policies which initiate Programmes of work."@en .



###  https://www.escidoc.org/ontologies/csmo/#PrincipleInvestigator

csmo:#PrincipleInvestigator rdf:type owl:Class ;
                            
                            rdfs:subClassOf csmo:#Investigator ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty csmo:#isPrincipalInvestigatorOf ;
                                              owl:onClass csmo:#Study ;
                                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                            ] ;
                            
                            owl:disjointWith csmo:#ResearchStudent .



###  https://www.escidoc.org/ontologies/csmo/#Programme

csmo:#Programme rdf:type owl:Class ;
                
                rdfs:label "Programme"@en ;
                
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty csmo:#isProgrammeOf ;
                                      owl:someValuesFrom csmo:#Policy
                                    ] ;
                
                rdfs:subClassOf owl:Thing ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty csmo:#isProgrammeOf ;
                                  owl:someValuesFrom csmo:#Institution
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty terms:title ;
                                  owl:cardinality "1"^^xsd:nonNegativeInteger
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty csmo:#hasStudy ;
                                  owl:someValuesFrom csmo:#Study
                                ] ;
                
                rdfs:comment "Programmes. Related studies that have a common theme which are usually funded and resourced directly or with an intermediary organisation under the rubric of the programme."@en ,
                             "Scientific research programme"@en .



###  https://www.escidoc.org/ontologies/csmo/#Project

csmo:#Project rdf:type owl:Class ;
              
              rdfs:subClassOf owl:Thing ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty csmo:#hasStudy ;
                                owl:someValuesFrom csmo:#Study
                              ] .



###  https://www.escidoc.org/ontologies/csmo/#Publication

csmo:#Publication rdf:type owl:Class ;
                  
                  rdfs:subClassOf owl:Thing ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty csmo:#derivedPublication ;
                                    owl:someValuesFrom rdfs:Literal
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty csmo:#link ;
                                    owl:someValuesFrom rdfs:Literal
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty csmo:#motivatingPublication ;
                                    owl:someValuesFrom rdfs:Literal
                                  ] ;
                  
                  rdfs:comment """bibo:Document
A document (noun) is a bounded physical representation of body of information designed with the capacity (and usually intent) to communicate. A document may manifest symbolic, diagrammatic or sensory-representational information."""@en .



###  https://www.escidoc.org/ontologies/csmo/#RawData

csmo:#RawData rdf:type owl:Class ;
              
              rdfs:subClassOf csmo:#Result .



###  https://www.escidoc.org/ontologies/csmo/#ResearchStudent

csmo:#ResearchStudent rdf:type owl:Class ;
                      
                      rdfs:subClassOf csmo:#Investigator .



###  https://www.escidoc.org/ontologies/csmo/#Result

csmo:#Result rdf:type owl:Class ;
             
             rdfs:label "Result"@en ;
             
             rdfs:subClassOf nfo:Folder ;
             
             rdfs:comment "Results deriving from the investigations usually proceed through different stages."@en .



###  https://www.escidoc.org/ontologies/csmo/#Sample

csmo:#Sample rdf:type owl:Class ;
             
             rdfs:label "Sample"@en ;
             
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty terms:title ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty csmo:#chemicalFormula ;
                               owl:someValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty csmo:#hasSampleParameter ;
                               owl:someValuesFrom csmo:#SampleParameter
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty terms:alternative ;
                               owl:someValuesFrom xsd:string
                             ] ;
             
             rdfs:comment "Information on the material sample under investigation within the study. "@en ;
             
             skos:note "Sample: information on the material sample under investigation within the study. The model has fields for a sample’s name, chemical formula and any associated special information about it, such as specific safety information on a toxic material. "@en .



###  https://www.escidoc.org/ontologies/csmo/#SampleParameter

csmo:#SampleParameter rdf:type owl:Class ;
                      
                      rdfs:label "SampleParameter"@en ;
                      
                      owl:equivalentClass [ rdf:type owl:Restriction ;
                                            owl:onProperty csmo:#isSampleParameterOf ;
                                            owl:someValuesFrom csmo:#Sample
                                          ] ;
                      
                      rdfs:subClassOf csmo:#Parameter .



###  https://www.escidoc.org/ontologies/csmo/#Simulation

csmo:#Simulation rdf:type owl:Class ;
                 
                 rdfs:label "Simulation"@en ;
                 
                 rdfs:subClassOf csmo:#Investigation ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty csmo:#hasParameter ;
                                   owl:someValuesFrom csmo:#Parameter
                                 ] ;
                 
                 rdfs:comment "Investigations that test a model of part of the world, and a computer simulation of the state space of that model. This will typically involve some simulation package with some initial parameters, and generate a dataset representing the result of the simulation."@en .



###  https://www.escidoc.org/ontologies/csmo/#Study

csmo:#Study rdf:type owl:Class ;
            
            rdfs:label "Study"@en ;
            
            rdfs:subClassOf owl:Thing ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty csmo:#hasInvestigation ;
                              owl:someValuesFrom csmo:#Investigation
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty csmo:#hasPurpose ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty csmo:#studyCreationDate ;
                              owl:someValuesFrom xsd:dateTimeStamp
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty terms:title ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty csmo:#hasStudyManager ;
                              owl:someValuesFrom foaf:Person
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty csmo:#isStudyOf ;
                              owl:onClass csmo:#Programme ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty csmo:#studyStatus ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty csmo:#relatedMaterial ;
                              owl:someValuesFrom xsd:string
                            ] ;
            
            rdfs:comment "Studies investigate some aspect of science and have a Principal Investigator and/or institution, co-investigators and some specific purpose. "@en ,
                         "Study is a body of scientific work on a particular subject of investigation"@en .





